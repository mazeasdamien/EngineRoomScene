<?xml version="1.0" encoding="UTF-8"?>
<dds xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://community.rti.com/schema/6.1.0/rti_dds_profiles.xsd" version="6.1.0">
    <qos_library name="Library">
      <qos_profile name="TxProfile" is_default_qos="true" is_default_participant_factory_profile="true">
        <?rti-qos_snippet?>
        <domain_participant_qos>
          <transport_builtin>
            <mask>UDPv4 | SHMEM</mask>
          </transport_builtin>
          <discovery>
            <initial_peers>
              <element>DPC000554.cns.cranfield.ac.uk</element>
              <element>LAP002408.cns.cranfield.ac.uk</element>
            </initial_peers>
          </discovery>
          <participant_name>
            <name>TxParticipant</name>
          </participant_name>
        </domain_participant_qos>
        <datareader_qos>
          <reliability>
            <kind>RELIABLE_RELIABILITY_QOS</kind>
            <max_blocking_time>
              <nanosec>100000000</nanosec>
            </max_blocking_time>
          </reliability>
          <history>
            <depth>10</depth>
          </history>
          <resource_limits>
            <max_samples>1024</max_samples>
            <max_instances>10</max_instances>
            <max_samples_per_instance>1024</max_samples_per_instance>
          </resource_limits>
        </datareader_qos>
        <datawriter_qos>
          <resource_limits>
            <max_samples>1024</max_samples>
            <max_instances>10</max_instances>
            <max_samples_per_instance>1024</max_samples_per_instance>
          </resource_limits>
          <publish_mode>
            <kind>ASYNCHRONOUS_PUBLISH_MODE_QOS</kind>
          </publish_mode>
        </datawriter_qos>
      </qos_profile>
    </qos_library>

</dds>
